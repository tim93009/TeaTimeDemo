@model List<Category>
<h1>類別清單</h1>
<table class="table table-bordered table-striped"
	<thead>
		<tr>
			<th>類別名稱</th>
			<th>顯示順序</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var obj in Model.OrderBy(u => u.DisplayOrder))
		{
				<tr>
						<td>@obj.Name</td>
						<td>@obj.DisplayOrder</td>
				</tr>
		}
	</tbody>
</table>